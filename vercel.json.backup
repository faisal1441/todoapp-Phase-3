{
  "version": 2,
  "builds": [
    {
      "src": "nextjs-app/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "asgi_app.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "asgi_app.py"
    },
    {
      "src": "/(.*)",
      "dest": "nextjs-app/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "/api"
  }
}


